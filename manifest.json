{
  "manifest_version": 2,
  "name": "Custom Shortcuts",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.4.1.min.js"]
    }
  ],
  "permissions": [
          "tabs",
          "storage"
  ],
  "browser_action": {
    "default_icon": "Shortcut.png",
    "default_popup": "popup.html"
  },   
  "background": {
    "scripts": ["background.js"]
  },
  "commands": {
    "Shortkey-1": {
      "description": "Shortcut 1"
    },
    "Shortkey-2": {
      "description": "Shortcut 2"
    },
    "Shortkey-3": {
      "description": "Shortcut 3"
    },
    "Shortkey-4": {
      "description": "Shortcut 4"
    },
    "Shortkey-5": {
      "description": "Shortcut 5"
    },
    "Shortkey-6": {
      "description": "Shortcut 6"
    },
    "Shortkey-7": {
      "description": "Shortcut 7"
    },
    "Shortkey-8": {
      "description": "Shortcut 8"
    }
  }
}

